{
  "description": "Modify's 'New Tab' page and adds utilities.",
  "manifest_version": 2,
  "name": "My New Tab",
  "version": "2.0.5",
  "homepage_url": "http://www.codeclark.tk",
  "browser_action": {
    "matches": ["*://*/_/chrome/newtab*"],
    "default_icon": "img/icon16.png",
    "default_popup": "popup.html",
    "default_title": "My New Tab"
  },
  "chrome_url_overrides": {
    "newtab": "myNewTab.html"
  },
  "icons": { 
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "https://api.unsplash.com/",
    "https://andruxnet-random-famous-quotes.p.mashape.com/",
    "storage"
  ]
}
